{
	"name": "BlueSpiceCountThings",
	"license-name": "GPL-3.0-only",
	"version": "3.0.0-beta",
	"url": "https://help.bluespice.com/index.php/CountThings",
	"author": [
		"Markus Glaser",
		"Mathias Scheer",
		"Peter Boehm"
	],
	"descriptionmsg": "bs-countthings-desc",
	"type": "bluespice",
	"requires": {
		"MediaWiki": ">= 1.31.0",
		"extensions": {
			"BlueSpiceFoundation": ">= 3.0.0"
		}
	},
	"attributes": {
		"BlueSpiceFoundation": {
			"Extensions": {
				"BlueSpiceCountThings": {
					"className": "CountThings",
					"extPath": "/BlueSpiceCountThings"
				}
			},
			"TagRegistry": {
				"countfiles": "BlueSpice\\CountThings\\Tag\\CountFiles",
				"countarticles": "BlueSpice\\CountThings\\Tag\\CountArticles",
				"countusers": "BlueSpice\\CountThings\\Tag\\CountUsers",
				"countcharacters": "BlueSpice\\CountThings\\Tag\\CountCharacters"
			},
			"RendererRegistry": {
				"countthings-countcharacters": "BlueSpice\\CountThings\\Renderer\\CountCharacters"
			}
		},
		"VisualEditor": {
			"PluginModules": [
				"ext.bluespice.countthings.countcharacters.visualEditor",
				"ext.bluespice.countthings.countusers.visualEditor",
				"ext.bluespice.countthings.countarticles.visualEditor",
				"ext.bluespice.countthings.countfiles.visualEditor"
			]
		}
	},
	"ResourceModules": {
		"ext.bluespice.countthings.countcharacters.visualEditor": {
			"scripts": [
				"ve/ve.ce.CountCharactersNode.js",
				"ve/ve.dm.CountCharactersNode.js",
				"ve/ve.ui.CountCharactersInspector.js",
				"ve/ve.ui.CountCharactersInspectorTool.js"
			],
			"targets": [
				"desktop",
				"mobile"
			],
			"messages": [
				"bs-countthings-ve-countthingsinspector-mode",
				"bs-countthings-ve-countcharacters-title"
			],
			"dependencies": [
				"ext.visualEditor.mwcore"
			]
		},
		"ext.bluespice.countthings.countusers.visualEditor": {
			"scripts": [
				"ve/ve.ce.CountUsersNode.js",
				"ve/ve.dm.CountUsersNode.js",
				"ve/ve.ui.CountUsersInspector.js",
				"ve/ve.ui.CountUsersInspectorTool.js"
			],
			"targets": [
				"desktop",
				"mobile"
			],
			"messages": [
				"bs-countthings-ve-countusers-title"
			],
			"dependencies": [
				"ext.visualEditor.mwcore"
			]
		},
		"ext.bluespice.countthings.countarticles.visualEditor": {
			"scripts": [
				"ve/ve.ce.CountArticlesNode.js",
				"ve/ve.dm.CountArticlesNode.js",
				"ve/ve.ui.CountArticlesInspector.js",
				"ve/ve.ui.CountArticlesInspectorTool.js"
			],
			"targets": [
				"desktop",
				"mobile"
			],
			"messages": [
				"bs-countthings-ve-countarticles-title"
			],
			"dependencies": [
				"ext.visualEditor.mwcore"
			]
		},
		"ext.bluespice.countthings.countfiles.visualEditor": {
			"scripts": [
				"ve/ve.ce.CountFilesNode.js",
				"ve/ve.dm.CountFilesNode.js",
				"ve/ve.ui.CountFilesInspector.js",
				"ve/ve.ui.CountFilesInspectorTool.js"
			],
			"targets": [
				"desktop",
				"mobile"
			],
			"messages": [
				"bs-countthings-ve-countfiles-title"
			],
			"dependencies": [
				"ext.visualEditor.mwcore"
			]
		}
	},
	"MessagesDirs": {
		"BlueSpiceCountThings": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"CountThings": "CountThings.class.php"
	},
	"AutoloadNamespaces": {
		"BlueSpice\\CountThings\\": "src/"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "BlueSpiceCountThings/resources"
	},
	"Hooks": {
		"BSInsertMagicAjaxGetData": [
			"\\BlueSpice\\CountThings\\Hook\\BSInsertMagicAjaxGetData\\CountArticles::callback",
			"\\BlueSpice\\CountThings\\Hook\\BSInsertMagicAjaxGetData\\CountUsers::callback",
			"\\BlueSpice\\CountThings\\Hook\\BSInsertMagicAjaxGetData\\CountFiles::callback",
			"\\BlueSpice\\CountThings\\Hook\\BSInsertMagicAjaxGetData\\CountCharacters::callback"
		]
	},
	"load_composer_autoloader" : true,
	"manifest_version": 2
}
